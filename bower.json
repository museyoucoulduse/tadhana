    , "feelings"
    , "scream"
    , "read"
    , "stories"
    , "rumors"
    , "gossips"
  ]
    , "homepage": "https://github.com/museyoucoulduse/tadhana"
    , "ignore": [
    "**/.*"
    , "node_modules"
    , "bower_components"
    , "test"
    , "tests"
  ]
    , "dependencies": {
        "bootstrap": "^4.0.0-alpha.4"
        , "angular": "1.5.8"
        , "font-awesome": "^4.6.3"
    }
    , "overrides": {
        "bootstrap": {
            "main": [
                "dist/js/bootstrap.min.js"
                , "scss/bootstrap.scss"
                , "dist/css/bootstrap.min.css"
            ]
        }
        , "font-awesome": {
            "main": [
                "less/font-awesome.less"
                , "scss/font-awesome.scss"
                , "css/font-awesome.min.css"
            ]
        }
    }
}