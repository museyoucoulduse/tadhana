{
    "name": "tadhana"
    , "description": "This is a website where you can shout out everything you ever wanted!"
    , "main": "index.js"
    , "authors": [
    "Simon Blaszczynski"
  ]
    , "license": "AGPL-3.0"
    , "keywords": [
    "shout"
    , "anger"
    , "love"
    , "problems"
    , "issues"
    , "plus"
    , "minus"
